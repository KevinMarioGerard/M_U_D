{
  "name": "M.U.D.",
  "version": "1.0",
  "description": "Malicious URL Detector!",
  "permissions": ["contextMenus", "tabs", "webNavigation", "storage", "declarativeContent", "<all_urls>"],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "M.U.D.",
    "default_popup": "popup.html",
    "default_icon": {
      "48": "images/logo.png"
    }
  },
  "icons": {
    "48": "images/logo.png"
  },
  "manifest_version": 2
}